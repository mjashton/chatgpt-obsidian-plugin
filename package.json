{
	"name": "chatgpt-to-obsidian",
	"version": "1.0.0",
	"description": "Import ChatGPT conversations and save individual responses as Obsidian notes",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "jest",
		"test:watch": "jest --watch",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"chatgpt",
		"ai",
		"notes"
	],
	"author": "Your Name",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^24.3.0",
		"@typescript-eslint/eslint-plugin": "^8.40.0",
		"@typescript-eslint/parser": "^8.40.0",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.25.9",
		"jest": "^30.0.5",
		"obsidian": "latest",
		"ts-jest": "^29.1.2",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2"
	}
}
